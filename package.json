{
  "name": "react-yearly-calendar",
  "version": "1.1.5",
  "description": "A yearly calendar built in React.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:BelkaLab/react-yearly-calendar"
  },
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rm -rf lib dist",
    "build": "babel src --out-dir lib",
    "prepublish": "NODE_ENV=production; npm run prettify; babel src --out-dir lib",
    "prettify": "prettier --config .prettierrc --write \"src/**/*.js\"",
    "lint": "eslint --config .eslintrc src/",
    "lint:fix": "eslint --fix --config .eslintrc src/"
  },
  "keywords": [
    "react",
    "react.js",
    "calendar",
    "year",
    "yearly",
    "react-component"
  ],
  "author": "Belka <giovanni@belka.us> (http://belka.us)",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.13.0",
    "react": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.7.4"
  }
}
